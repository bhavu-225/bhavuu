print(f.read())
f.close()
# ------------------------------
# read specific line
f=open("C:/Users/priya/OneDrive/Desktop/bhavuu/file read.txt","r")
print(f.readline())
f.close()
# ----------------------------------
# read all lines
f=open("C:/Users/priya/OneDrive/Desktop/bhavuu/file read.txt","r")
print(f.readlines())
f.close()
